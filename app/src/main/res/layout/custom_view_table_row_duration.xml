<?xml version="1.0" encoding="utf-8"?>
    <merge xmlns:android="http://schemas.android.com/apk/res/android">
<TableRow
    android:id="@+id/CustomView_root_tablerow"
style="@style/MainDataFragmentTableRow"
    >

    <TextView
        style="@style/MainDataFragmentFirstColumnHeadings"
        android:id="@+id/CustomView_heading_textview"
        android:text="a"
        />

    <LinearLayout
        style="@style/MainDataFragmentSecondColumn"
        android:gravity="center"
        android:orientation="vertical"
        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:id="@+id/CustomViewDuration_start_time"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/CustomViewDuration_chronometer_textview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/CustomViewDuration_end_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="gone"
            />

    </LinearLayout>
</TableRow>
</merge>
